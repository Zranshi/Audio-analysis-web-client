   {\n            startValue: \"0\",\n          },\n          {\n            type: \"inside\",\n          },\n        ],\n        toolbox: {\n          left: \"center\",\n          feature: {\n            dataZoom: {\n              yAxisIndex: \"none\",\n            },\n            restore: {},\n            saveAsImage: {},\n          },\n        },\n        xAxis: {\n          name: \"时间\",\n          data: this.x,\n        },\n        yAxis: {\n          name: '幅度'\n        },\n        series: [\n          {\n            //显示数据的名称\n            name: \"dB\",\n            //平滑曲线\n            smooth: true,\n            type: \"line\",\n            data: this.data,\n          },\n        ],\n      };\n      myChart.setOption(option);\n    },\n  },\n  mounted() {\n    this.myEcharts();\n  },\n};\n</script>\n\n<style scoped>\n\n</style>"]}]}