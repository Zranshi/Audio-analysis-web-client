     type: \"inside\",\n          },\n          ],\n          toolbox: {\n            left: \"right\",\n            feature: {\n              saveAsImage: {},\n            },\n          },\n          legend: {\n            data: ['dB(左声道)', 'dB(右声道)']\n          },\n          xAxis: {\n            name: \"时间/ms\",\n            data: this.x,\n          },\n          yAxis: {\n            name: '幅度'\n          },\n          series: [{\n            name: \"dB(左声道)\",\n            smooth: true,\n            type: \"line\",\n            data: this.data_l,\n          }, {\n            name: \"dB(右声道)\",\n            smooth: true,\n            type: \"line\",\n            data: this.data_r,\n          },\n          ],\n        };\n      },\n    },\n  },\n  methods: {\n    init() {\n      this.chart = echarts.init(document.getElementById(\"main\"));\n      window.addEventListener('resize', this.chart.resize)\n      this.chart.setOption(this.option);\n    },\n  },\n  mounted() {\n    this.init();\n  },\n  watch: {\n    data_l: {\n      handler() {\n        this.option.series[0].data = this.data_l;\n        this.option.series[1].data = this.data_r;\n        this.option.xAxis.data = this.x;\n        this.chart.setOption(this.option)\n      },\n      deep: true,\n      immediate: true\n    },\n  },\n};\n</script>\n\n<style scoped>\n\n</style>"],"sourceRoot":"src/components"}]}